{
  "name": "nestjs-extra",
  "version": "0.0.3",
  "description": "NestJS extra modules to speed up development",
  "author": "aleksandar@tuta.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "rimraf dist && tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix src --ext .js,.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "init-deps": "sh peerDeps.sh"
  },
  "dependencies": {
    "@nestjs/bull": "^0.1.1",
    "@nestjs/common": "7.0.0",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/typeorm": "7.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.12.2",
    "@types/cron": "^1.7.2",
    "@types/deep-diff": "^1.0.0",
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.2.1",
    "@types/minio": "^7.0.5",
    "@types/node": "^13.13.4",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/sharp": "^0.25.0",
    "@types/sqlstring": "^2.2.1",
    "aws-sdk": "^2.668.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.13.0",
    "casbin": "^4.7.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "deep-diff": "^1.0.2",
    "faker": "^4.1.0",
    "handlebars": "^4.7.6",
    "moment": "^2.25.1",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.3",
    "query-string": "^6.12.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "sharp": "^0.25.2",
    "sqlstring": "^2.3.2",
    "typeorm": "^0.2.7",
    "typeorm-naming-strategies": "^1.1.0",
    "useragent": "^2.3.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/core": "^7.0.9",
    "@nestjs/platform-express": "^7.0.9",
    "@nestjs/typeorm": "^7.0.0",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.9",
    "@types/express": "^4.17.6",
    "@types/useragent": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  }
